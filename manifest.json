{
  "manifest_version": 3,
  "name": "四分图 (QuadSplit)",
  "version": "1.0",
  "description": "一键将图片分割成四个相等部分的Chrome扩展",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "downloads",
    "storage"
  ]
}
